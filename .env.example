# ET Agency - Environment Variables Example
# Copy this file to .env and fill in your actual values

# App Configuration
REACT_APP_URL=https://etagency.cl
REACT_APP_ENVIRONMENT=development

# API Configuration
REACT_APP_API_URL=https://api.etagency.cl
REACT_APP_API_TIMEOUT=10000
REACT_APP_API_RETRIES=3
REACT_APP_API_VERSION=v1

# Authentication
REACT_APP_SESSION_TIMEOUT=3600000

# Analytics
REACT_APP_GA_ID=G-XXXXXXXXXX
REACT_APP_GTM_ID=GTM-XXXXXXX
REACT_APP_FB_PIXEL_ID=123456789
REACT_APP_HOTJAR_ID=1234567

# Social Media
REACT_APP_FACEBOOK_APP_ID=123456789

# Feature Flags
REACT_APP_CHATBOT_ENABLED=true
REACT_APP_BLOG_ENABLED=true
REACT_APP_PORTFOLIO_ENABLED=true
REACT_APP_TESTIMONIALS_ENABLED=true
REACT_APP_NEWSLETTER_ENABLED=true
REACT_APP_DARK_MODE_ENABLED=true
REACT_APP_ANIMATIONS_ENABLED=true
REACT_APP_PWA_ENABLED=false

# File Upload
REACT_APP_UPLOAD_URL=/api/upload

# Maps
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Email Service (EmailJS)
REACT_APP_EMAILJS_SERVICE_ID=your_service_id
REACT_APP_EMAILJS_TEMPLATE_ID=your_template_id
REACT_APP_EMAILJS_PUBLIC_KEY=your_public_key

# Configuración del servidor backend
PORT=5000
NODE_ENV=development

# Configuración de Mailgun (Backend)
MAILGUN_API_KEY=your_mailgun_api_key
MAILGUN_DOMAIN=your_mailgun_domain
MAILGUN_FROM_EMAIL=noreply@yourdomain.com
MAILGUN_TO_EMAIL=info@yourdomain.com

# Configuración de CORS
CORS_ORIGIN=http://localhost:5174

# Configuración de la aplicación
APP_NAME=ET Agency
APP_URL=https://yourdomain.com

# reCAPTCHA
REACT_APP_RECAPTCHA_SITE_KEY=your_recaptcha_site_key
REACT_APP_RECAPTCHA_ENABLED=false

# Development
REACT_APP_MOCK_API=false
REACT_APP_SHOW_PERFORMANCE=false

# Error Reporting
REACT_APP_SENTRY_DSN=your_sentry_dsn_here
REACT_APP_LOG_LEVEL=error

# Build Configuration
GENERATE_SOURCEMAP=false
REACT_APP_VERSION=$npm_package_version

# Performance
REACT_APP_BUNDLE_ANALYZER=false
REACT_APP_PRELOAD_FONTS=true

# Security
REACT_APP_CSP_ENABLED=true
REACT_APP_HTTPS_ONLY=false

# Cache
REACT_APP_CACHE_ENABLED=true
REACT_APP_CACHE_TTL=300000

# Monitoring
REACT_APP_MONITORING_ENABLED=false
REACT_APP_PERFORMANCE_THRESHOLD=3000

# Third Party Services
REACT_APP_STRIPE_PUBLIC_KEY=pk_test_your_stripe_key
REACT_APP_PAYPAL_CLIENT_ID=your_paypal_client_id

# Content Management
REACT_APP_CMS_URL=https://cms.etagency.cl
REACT_APP_CMS_API_KEY=your_cms_api_key

# CDN
REACT_APP_CDN_URL=https://cdn.etagency.cl
REACT_APP_ASSETS_URL=https://assets.etagency.cl

# Database (if using client-side DB)
REACT_APP_DB_URL=your_database_url
REACT_APP_DB_API_KEY=your_database_api_key

# WebSocket
REACT_APP_WS_URL=wss://ws.etagency.cl

# Push Notifications
REACT_APP_VAPID_PUBLIC_KEY=your_vapid_public_key

# Chat/Support
REACT_APP_CHAT_WIDGET_ID=your_chat_widget_id
REACT_APP_INTERCOM_APP_ID=your_intercom_app_id
REACT_APP_ZENDESK_KEY=your_zendesk_key

# A/B Testing
REACT_APP_OPTIMIZELY_SDK_KEY=your_optimizely_sdk_key
REACT_APP_SPLIT_IO_KEY=your_split_io_key

# Localization
REACT_APP_LOCALIZATION_PROJECT_ID=your_localization_project_id
REACT_APP_LOCALIZATION_API_KEY=your_localization_api_key

# Video/Media
REACT_APP_VIMEO_ACCESS_TOKEN=your_vimeo_token
REACT_APP_YOUTUBE_API_KEY=your_youtube_api_key
REACT_APP_CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name
REACT_APP_CLOUDINARY_API_KEY=your_cloudinary_api_key

# Search
REACT_APP_ALGOLIA_APP_ID=your_algolia_app_id
REACT_APP_ALGOLIA_SEARCH_KEY=your_algolia_search_key

# Forms
REACT_APP_TYPEFORM_ID=your_typeform_id
REACT_APP_HUBSPOT_PORTAL_ID=your_hubspot_portal_id

# Legal/Compliance
REACT_APP_PRIVACY_POLICY_URL=https://etagency.cl/privacy
REACT_APP_TERMS_OF_SERVICE_URL=https://etagency.cl/terms
REACT_APP_COOKIE_POLICY_URL=https://etagency.cl/cookies

# Backup/Sync
REACT_APP_BACKUP_ENABLED=false
REACT_APP_SYNC_INTERVAL=300000

# Testing
REACT_APP_TEST_MODE=false
REACT_APP_MOCK_DELAY=1000

# Custom Features
REACT_APP_CUSTOM_FEATURE_1=false
REACT_APP_CUSTOM_FEATURE_2=false
REACT_APP_CUSTOM_FEATURE_3=false